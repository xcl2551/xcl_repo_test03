<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.pccw.ucenter.mapper.ScdaUserAuthMapper" >


    <select id="findByUsername" parameterType="java.lang.String" resultType="com.pccw.framework.domain.ucenter.pojo.ScdaUserAuth">
        select * from Scda_UserAuth   where Scda_UserAuth.username = #{username}
    </select>

</mapper>